/* lethead1.c */
#include <stdio.h>
#define NAME "GiGATHINK, INC."
#define ADDRESS "101 Megabuck Plaza"
#define PLACE "Megapolis, CA 94904"
#define WIDTH 40

void starbar(void);

int main(void)
{
    starbar();
    printf("%s\n", NAME);
    printf("%s\n", ADDRESS);
    printf("%s\n", PLACE);
    starbar();

    return 0;
}

void starbar(void)
{
    int i;
    for (i = 0; i < 40; i++)
	putchar('*');
    putchar('\n');
}
